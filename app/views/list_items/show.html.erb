<p style="color: green"><%= notice %></p>

<table class="table">
  <thead>
  <tr>
    <th scope="col">Name</th>
    <th scope="col">Quantity</th>
  </tr>
  </thead>
  <tbody class="table-group-divider">
  <%= render @list_item %>
  </tbody>
</table>

<div>
  <%= link_to "Edit this list item", edit_list_list_item_path(@list, @list_item) %> |
  <%= link_to "Back to list items", list_list_items_path(@list) %>

  <%#= button_to "Destroy this list item", [@list, @list_item] , method: :delete, data: { confirm: 'Are you sure?' }  %>
</div>
